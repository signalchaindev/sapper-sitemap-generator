{
  "name": "",
  "description": "",
  "version": "0.0.1",
  "license": "",
  "scripts": {
    "dev": "sapper dev",
    "dev:watch": "run-p --race watch:dirs dev",
    "tempo": "nodemon -r esm __tempo__/watcher.js --ignore __tempo__",
    "build": "npm run generate:prod && cross-env NODE_ENV=production sapper build",
    "export": "npm run generate:prod && cross-env NODE_ENV=production sapper export",
    "clean": "rm -rf __sapper__ src/node_modules",
    "debug": "concurrently \"sapper dev\" \"npm run browser-sync\" --names \"APP,SYNC\"",
    "browser-sync": "browser-sync start --proxy http://localhost:3000/ --port 3001 --ui-port 3002 --files './src/**/*.js, ./src/**/*.svelte, ./src/**/*.html, ./src/**/*.css, ./src/**/*.scss' --no-open --no-notify --reload-delay=500",
    "watch:dirs": "cross-env NODE_ENV=development nodemon -r esm sapper-watcher.js --watch src/styles/theme --watch src/content",
    "generate:dev": "cross-env NODE_ENV=development node -r esm generator/index.js",
    "generate:prod": "cross-env NODE_ENV=production node -r esm generator/index.js"
  },
  "browserslist": [
    ">1%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "apollo-server": "2.17.0",
    "apollo-server-express": "2.17.0",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "cross-env": "7.0.2",
    "esm": "3.2.25",
    "express": "4.17.1",
    "graphql": "15.3.0",
    "node-fetch": "2.6.1",
    "sirv": "1.0.6"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/runtime": "7.11.2",
    "@graphql-tools/merge": "6.2.1",
    "@rollup/plugin-babel": "5.2.0",
    "@rollup/plugin-commonjs": "15.0.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "chalk": "4.1.0",
    "chokidar": "3.4.2",
    "dotenv": "8.2.0",
    "eslint": "7.8.1",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-cypress": "2.11.1",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-json": "2.1.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-svelte3": "2.7.3",
    "highlight.js": "10.1.2",
    "lodash.merge": "4.6.2",
    "marked": "1.1.1",
    "nodemon": "2.0.4",
    "npm-run-all": "4.1.5",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.1.1",
    "rollup": "2.26.10",
    "rollup-plugin-json": "4.0.0",
    "rollup-plugin-svelte": "6.0.0",
    "rollup-plugin-terser": "7.0.2",
    "sapper": "0.28.4",
    "sass": "1.26.10",
    "svelte": "3.24.1",
    "svelte-preprocess": "4.2.1"
  }
}